{
  "canisters": {
    "micha_frontend": {
      "source": [
        "src/micha_frontend/dist"
      ],
      "type": "assets",
      "build": "cd src/micha_frontend && npm run build"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "networks": {
    "local": {
      "bind": "127.0.0.1:4943",
      "type": "ephemeral"
    },
    "ic": {
      "providers": ["https://ic0.app"],
      "type": "persistent"
    }
  },
  "output_env_file": ".env",
  "version": 1
}
